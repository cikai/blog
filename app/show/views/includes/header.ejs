<script type="text/javascript">
    $(function() {
        $.ajax({
            type:"post",
            dataType:"json",
            url:"/index/header",
            success:function(data) {
                if(data && data instanceof Array) {
                    var arr = [];
                    data.forEach(function(item) {
                        var $element = $("<li><a href='#'>Link</a></li>");
                        $element.children().attr("href","/show/lists?forumId="+item._id);
                        $element.children().text(item.name);
                        arr.push($element);
                    })
                    $("ul.navbar-nav").prepend(arr);
                }
            }
        });
    })
</script>
